    "yup": "^0.32.11",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.12",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.0",
    "tailwindcss": "^2.2.19",
    "type": "module",
    "typescript": "4.5.2"
  },
  "include": [
    "./src/**/*"
  ],
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react"
  }
}
